      <% for(var i=0; i<users.length; i++) {%>
        <li class="users">
          <h3><img src="<%= user.profileImg %>" alt="user profile picture"></h3>
          <h1><a href="/profile/<%=user[i]._id%>"><%= user[i].name %></a></h1>
          <h6><%= user[i].location %></h6>
          <h3><%= user[i].memoir %></h3>
          <h2 id="email"><a href="https://mail.google.com/mail/?view=cm&fs=1&to="><%= user[i].email %></a></h2>

        </li>
        <% } %>
    </ul>
